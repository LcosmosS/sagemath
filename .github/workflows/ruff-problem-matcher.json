{
    "problemMatcher": [
      {
        "owner": "ruff-error",
        "severity": "error",
        "pattern": [
          {
            "regexp": "^(.*):(\\d+):(\\d+):\\s(?:E|F|PLE)\\d{3,4}\\s.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "message": 4
          }
        ]
      },
      {
        "owner": "ruff-warning",
        "severity": "warning",
        "pattern": [
          {
            "regexp": "^(.*):(\\d+):(\\d+):\\s(?:C|D|N|W|PLR|PLW|PLC)\\d{3,4}\\s.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "message": 4
          }
        ]
      }
    ]
  }
  